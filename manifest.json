{
    "author": "Xiaoqian Xiao",
    "command": "poetry run python run.py",
     "inputs": {
        "pipelines": {
            "description": "Name of pipelines used for denoising, can be both file of json files with pipeline or file with name of pipelines from package",
            "base": "file",
            "optional": true
        },
      "api-key": {
          "base": "api-key"
        }
    },
    "config": {
        "using custom-pipline": {
            "default": false,
            "description": "check if you want to custom the denoise pipeline here",
            "type": "boolean"
        },
        "name": {
            "default": "custom-pipline",
            "description": "pipline set by gear user",
            "type": "string"
        },
        "description": {
            "default": "pipline set by gear user",
            "description": "descrip this pipline",
            "type": "string"
        },
        "wm": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "wm-temp_deriv": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "wm-quad_terms": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "csf": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "csf-temp_deriv": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "csf-quad_terms": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "gs": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "gs-temp_deriv": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "gs-quad_terms": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "acompcor": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "aroma": {
            "default": false,
            "description": "run anatomical workflows only",
            "type": "boolean"
        },
        "fd_th": {
            "default": 0.5,
            "description": "framewise_displacement threshold, numeric which scans have fd greater thant it will be identified as outliers.",
            "type": "integer"
        },
        "dvars_th": {
            "default": 3,
            "description": "std_dvars threshold, numeric which scans have std_dvars greater thant it will be identified as outliers.",
            "type": "integer"
        }
    },
    "custom": {
        "docker-image": "xiaoqianxiao/test:latest",
        "flywheel": {
            "suite": "BIDS Apps"
        },
        "gear-builder": {
            "category": "analysis",
            "image": "xiaoqianxiao/test:latest"
        },
        "license": {
            "non-commercial-use-only": true
        }
    },
    "description": "",
    "environment": {       
        "FLYWHEEL": "/flywheel/v0",
        "PWD": "/flywheel/v0"
    },
    "label": "RestingfMRI_Denoise",
    "license": "",
    "maintainer": "Xiaoqian <xiaoqian@stanford.edu>",
    "name": "RestingfMRI_Denoise",
    "source": "https://github.com/XiaoXiaoqian/RestingfMRI_Denoise.git",
    "url": "https://github.com/flywheel-apps/bids-fmriprep/blob/master/README.md",
    "version": "0.1"
}
